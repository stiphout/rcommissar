<h1>Editing product</h1>

<% form_for(@product) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
    <p>
      <%= f.label :artist_name %><br />
      <%= f.text_field :artist_name %>
    </p>
    <p>
      <%= f.label :version_title %><br />
      <%= f.text_field :version_title %>
    </p>
    <p>
      <%= f.label :internal_version_title %><br />
      <%= f.text_field :internal_version_title %>
    </p>
    <p>
      <%= f.label :format_description %><br />
      <%= f.text_field :format_description %>
    </p>
  <p>
    <%= f.label :icpn %><br />
    <%= f.text_field :icpn %>
  </p>
    <p>
      <%= f.label :genre %><br />
      <%= f.text_field :genre %>
    </p>
    <p>
      <%= f.label :label %><br />
      <%= f.text_field :label %>
    </p>
    <p>
      <%= f.label :marketing_intent %><br />
      <%= f.text_field :marketing_intent %>
    </p>
    <p>
      <%= f.label :copyright_company %><br />
      <%= f.text_field :copyright_company %>
    </p>
    <p>
      <%= f.label :copyright_year %><br />
      <%= f.text_field :copyright_year %>
    </p>
    <p>
      <%= f.label :release_date %><br />
      <%= f.text_field :release_date %>
    </p>
    <p>
      <%= f.label :ownership_type %><br />
      <%= f.text_field :ownership_type %>
    </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @product %> |
<%= link_to 'Back', products_path %>
